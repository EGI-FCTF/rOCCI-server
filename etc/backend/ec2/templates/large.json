{
    "mixins":[
        {
            "term":"large",
            "scheme":"http://aws.amazon.com/ec2/instance-types#",
            "title":"Large Instance",
            "related":[
                "http://schemas.ogf.org/occi/infrastructure#resource_tpl"
            ],
            "attributes":{
                "occi":{
                    "compute":{
                        "architecture":{
                            "Mutable":false,
                            "Type":"string",
                            "Default":"x64"
                        },
                        "cores":{
                            "Mutable":false,
                            "Type":"number",
                            "Default":2
                        },
                        "speed":{
                            "Mutable":false,
                            "Type":"number",
                            "Default":2
                        },
                        "memory":{
                            "Mutable":false,
                            "Type":"number",
                            "Default":7.5
                        }
                    },
                    "storage":{
                        "size":{
                            "Mutable":false,
                            "Type":"number",
                            "Default":850
                        }
                    }
                }
            }
        }
    ]
}