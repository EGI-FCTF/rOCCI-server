{
  "mixins":[
    {
      "term":"extra_large",
      "scheme":"http://occi.example.org/occi/infrastructure/resource_tpl#",
      "title":"Extra Large Instance",
      "location":"/mixin/resource_tpl/extra_large/",
      "depends":[
        "http://schemas.ogf.org/occi/infrastructure#resource_tpl"
      ],
      "applies":[
        "http://schemas.ogf.org/occi/infrastructure#compute"
      ],
      "attributes":{
        "occi":{
          "compute":{
            "architecture":{
              "Mutable":false,
              "Type":"string",
              "Default":"x64"
            },
            "cores":{
              "Mutable":false,
              "Type":"number",
              "Default":4
            },
            "speed":{
              "Mutable":false,
              "Type":"number",
              "Default":1
            },
            "memory":{
              "Mutable":false,
              "Type":"number",
              "Default":15
            }
          }
        }
      }
    },
    {
      "term":"large",
      "scheme":"http://occi.example.org/occi/infrastructure/resource_tpl#",
      "title":"Large Instance",
      "location":"/mixin/resource_tpl/large/",
      "depends":[
        "http://schemas.ogf.org/occi/infrastructure#resource_tpl"
      ],
      "applies":[
        "http://schemas.ogf.org/occi/infrastructure#compute"
      ],
      "attributes":{
        "occi":{
          "compute":{
            "architecture":{
              "Mutable":false,
              "Type":"string",
              "Default":"x64"
            },
            "cores":{
              "Mutable":false,
              "Type":"number",
              "Default":2
            },
            "speed":{
              "Mutable":false,
              "Type":"number",
              "Default":1
            },
            "memory":{
              "Mutable":false,
              "Type":"number",
              "Default":7.5
            }
          }
        }
      }
    },
    {
      "term":"medium",
      "scheme":"http://occi.example.org/occi/infrastructure/resource_tpl#",
      "title":"Medium Instance",
      "location":"/mixin/resource_tpl/medium/",
      "depends":[
        "http://schemas.ogf.org/occi/infrastructure#resource_tpl"
      ],
      "applies":[
        "http://schemas.ogf.org/occi/infrastructure#compute"
      ],
      "attributes":{
        "occi":{
          "compute":{
            "architecture":{
              "Mutable":true,
              "Required":false,
              "Type":"string",
              "Pattern":"x86|x64",
              "Default":"x86"
            },
            "cores":{
              "Mutable":false,
              "Type":"number",
              "Default":2
            },
            "speed":{
              "Mutable":false,
              "Type":"number",
              "Default":1
            },
            "memory":{
              "Mutable":false,
              "Type":"number",
              "Default":3.75
            }
          }
        }
      }
    },
    {
      "term":"small",
      "scheme":"http://occi.example.org/occi/infrastructure/resource_tpl#",
      "title":"Small Instance",
      "location":"/mixin/resource_tpl/small/",
      "depends":[
        "http://schemas.ogf.org/occi/infrastructure#resource_tpl"
      ],
      "applies":[
        "http://schemas.ogf.org/occi/infrastructure#compute"
      ],
      "attributes":{
        "occi":{
          "compute":{
            "architecture":{
              "Mutable":true,
              "Required":false,
              "Type":"string",
              "Pattern":"x86|x64",
              "Default":"x86"
            },
            "cores":{
              "Mutable":false,
              "Type":"number",
              "Default":1
            },
            "speed":{
              "Mutable":false,
              "Type":"number",
              "Default":1
            },
            "memory":{
              "Mutable":false,
              "Type":"number",
              "Default":1.7
            }
          }
        }
      }
    }
  ]
}
