{
  "mixins":[
    {
      "term":"Basic_A0",
      "scheme":"http://schemas.azure.microsoft.com/occi/infrastructure/resource_tpl#",
      "title":"Basic_A0 - 1 core and 0.75 GB RAM",
      "location":"/mixin/resource_tpl/Basic_A0/",
      "depends":[
        "http://schemas.ogf.org/occi/infrastructure#resource_tpl"
      ],
      "applies":[
        "http://schemas.ogf.org/occi/infrastructure#compute"
      ],
      "attributes":{
        "occi":{
          "compute":{
            "architecture":{
              "Mutable":true,
              "Required":false,
              "Type":"string",
              "Pattern":"x86|x64",
              "Default":"x64"
            },
            "cores":{
              "Mutable":false,
              "Type":"number",
              "Default":1
            },
            "speed":{
              "Mutable":false,
              "Type":"number",
              "Default":1
            },
            "memory":{
              "Mutable":false,
              "Type":"number",
              "Default":0.75
            }
          }
        }
      }
    }
  ]
}
